digraph shell {
    dummy -> cmd -> script
    dummy [
           name="dummy"
           type="dummy"
           ]
    cmd [
           name="cmd"
           type="shell"
           output="/tmp/hpipe/examples/shell/output/${bizdate}/cmd"
           cmd="echo ${bizdate}"
           ]
    script [
           name="script"
           type="shell"
           output="/tmp/hpipe/examples/shell/output/${bizdate}/script"
           script="script.sh ${bizdate} 'space and' 'XX YY' xxx-asdf_rr.sh"
           ]
}
