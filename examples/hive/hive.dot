digraph odps {
    step1 [
                name="step1"
                type="hive"
                vars="$bizdate=${date:YYYYMMDD}"
                output="/tmp/hpipe/examples/odps/output/${bizdate}/step1"
                hql="select * from tb_game where date='${bizdate}';"
                ]
    dummy [
           name="dummy"
           type="dummy"
           ]
    dummy -> step1
}
