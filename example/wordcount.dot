digraph wordcount {
    wordcount1 [
                name="wordcount1"
                type="shell"
                vars="$date=${YYYYMMDD}"
                script="wordcount/wordcount1.sh"
                ]
    wordcount2 [
                name="wordcount2"
                type="shell"
                vars="$date=$bizdate"
                script="wordcount/wordcount1.sh"
                ]
    dummy [
           name="dummy"
           type="dummy"
           vars="$bizdate=${YYYYMMDD}+2*$day"
           ]
    dummy -> wordcount1
    dummy -> wordcount2
}
